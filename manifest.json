{
  "manifest_version": 2,
  "name": "Alfa summariser",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "*://my.alfabank.com.ua/*"
      ],
      "js": ["jquery.min.js", "content.js"],
      "css":        ["table.css"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}